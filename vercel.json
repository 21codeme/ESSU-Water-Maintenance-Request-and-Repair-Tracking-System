{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/admin/:path*",
      "destination": "/frontend/admin/:path*"
    },
    {
      "source": "/dashboard/:path*",
      "destination": "/frontend/dashboard/:path*"
    },
    {
      "source": "/login/:path*",
      "destination": "/frontend/login/:path*"
    },
    {
      "source": "/myreports/:path*",
      "destination": "/frontend/myreports/:path*"
    },
    {
      "source": "/report/:path*",
      "destination": "/frontend/report/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/frontend/images/:path*"
    },
    {
      "source": "/",
      "destination": "/frontend/index.html"
    }
  ]
}
