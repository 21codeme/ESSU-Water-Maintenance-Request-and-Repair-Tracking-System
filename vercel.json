{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/frontend/admin/$1"
    },
    {
      "source": "/dashboard/(.*)",
      "destination": "/frontend/dashboard/$1"
    },
    {
      "source": "/login/(.*)",
      "destination": "/frontend/login/$1"
    },
    {
      "source": "/myreports/(.*)",
      "destination": "/frontend/myreports/$1"
    },
    {
      "source": "/report/(.*)",
      "destination": "/frontend/report/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/frontend/images/$1"
    },
    {
      "source": "/(.*\\.(js|css|html|png|jpg|jpeg|gif|svg|ico|json))",
      "destination": "/frontend/$1"
    },
    {
      "source": "/",
      "destination": "/frontend/index.html"
    }
  ]
}
