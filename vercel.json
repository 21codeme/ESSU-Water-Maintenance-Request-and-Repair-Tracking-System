{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/frontend/admin/:path*"
    },
    {
      "source": "/dashboard/:path*",
      "destination": "/frontend/dashboard/:path*"
    },
    {
      "source": "/login/:path*",
      "destination": "/frontend/login/:path*"
    },
    {
      "source": "/myreports/:path*",
      "destination": "/frontend/myreports/:path*"
    },
    {
      "source": "/report/:path*",
      "destination": "/frontend/report/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/frontend/images/:path*"
    },
    {
      "source": "/styles.css",
      "destination": "/frontend/styles.css"
    },
    {
      "source": "/app.js",
      "destination": "/frontend/app.js"
    },
    {
      "source": "/supabase-helpers.js",
      "destination": "/frontend/supabase-helpers.js"
    },
    {
      "source": "/",
      "destination": "/frontend/index.html"
    }
  ]
}
